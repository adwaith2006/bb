'center',\r\n          }}></Image>\r\n        <TouchableOpacity\r\n           style={{\r\n           width: RFValue(200),\r\n            height: RFValue(30),\r\n            backgroundColor: 'black',\r\n            alignSelf:'center', \r\n            marginTop:20,\r\n            marginBottom:15\r\n\r\n          }}\r\n          onPress={() => this.goToKoalaScreen()}>\r\n          <Text style={styles.Text2}>KOALA</Text>\r\n        </TouchableOpacity>\r\n        <Image\r\n          source={require('../images/quokka.jpg')}\r\n          style={{\r\n            width: null,\r\n            height: RFValue(250),\r\n            resizeMode: 'center',\r\n          }}></Image>\r\n        <TouchableOpacity\r\n         style={{\r\n            width: RFValue(200),\r\n            height: RFValue(30),\r\n            backgroundColor: 'black',\r\n            alignSelf:'center',\r\n            marginTop:20,\r\n            marginBottom:20\r\n\r\n          }}\r\n           onPress={() => this.goToQuokkaScreen()}>\r\n          <Text style={styles.Text2}>QUOKKA</Text>\r\n        </TouchableOpacity>\r\n        <Image\r\n          source={require('../images/echi.jpg')}\r\n          style={{\r\n            width: RFPercentage(30),\r\n            height: RFPercentage(30),\r\n            resizeMode: 'stretch',\r\n            alignSelf:'center',\r\n          }}></Image>\r\n        <TouchableOpacity\r\n          style={{\r\n            width: RFValue(200),\r\n            height:RFValue(30),\r\n            backgroundColor: 'black',\r\n            alignSelf:'center',\r\n            marginTop:20,\r\n            \r\n\r\n          }}\r\n          onPress={() => this.goToEchidnaScreen()}>\r\n          <Text style={styles.Text2}>ECHIDNA</Text>\r\n        </TouchableOpacity>\r\n          <TouchableOpacity\r\n          style={{\r\n            alignSelf: 'center',\r\n            marginTop: 50,\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'green',\r\n