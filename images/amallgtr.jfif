eScreen');\r\n  };\r\n  render() {\r\n    return (\r\n      <ScrollView style={styles.Container}>\r\n        <Text\r\n          style={{\r\n            fontFamily: 'sans-serif-condensed',\r\n            fontWeight: 'bold',\r\n            alignSelf: 'center',\r\n            justifyContent: 'center',\r\n            fontSize: 30,\r\n            color: 'black',\r\n          }}>\r\n          EUROPE\r\n        </Text>\r\n        <Text style={{ color: 'blue', fontSize: RFValue(20) }}>\r\n          Click on the name of the animal you wish to view{' '}\r\n        </Text>\r\n        \r\n        <Image\r\n          source={require('../images/bbear.jpg')}\r\n          style={{\r\n           width: RFValue(200),\r\n            height: RFValue(260),\r\n            resizeMode: 'stretch',\r\n            alignSelf:'center'\r\n          }}></Image>\r\n        <TouchableOpacity\r\n           style={{\r\n            width: RFValue(200),\r\n            height: RFValue(30),\r\n            backgroundColor: 'black',\r\n            alignSelf:'center',\r\n            marginTop: 15,\r\n            marginBottom: 15,\r\n            alignItems:'center'\r\n\r\n          }}\r\n          onPress={() => this.goToBearScreen()}>\r\n          <Text style={styles.Text}>BEAR</Text>\r\n        </TouchableOpacity>\r\n        <Image\r\n          source={require('../images/bison.jpg')}\r\n          style={{\r\n            width: RFValue(260),\r\n            height: RFValue(220),\r\n            resizeMode: 'stretch',\r\n            alignSelf:'center'\r\n          }}></Image>\r\n        <TouchableOpacity\r\n           style={{\r\n            width: RFValue(200),\r\n            height: RFValue(30),\r\n            backgroundColor: 'black',\r\n            alignSelf:'center',\r\n            marginTop: 15,\r\n            marginBottom: 15,\r\n            alignItems:'center'\r\n\r\n          }}\r\n          onPress={() => this.goToBisonScreen()}>\r\n          <Text style={styles.Text}>BISON</Text>\r\n        </Toucha