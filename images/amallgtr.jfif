{ RFPercentage, RFValue } from 'react-native-responsive-fontsize';\r\n\r\nexport default class AfricaScreen extends React.Component {\r\n  goToMapScreen = () => {\r\n    this.props.navigation.navigate('MapScreen');\r\n  };\r\n  goToHippoScreen = () => {\r\n    this.props.navigation.navigate('HippopotamusScreen');\r\n  };\r\n  goToGiraffeScreen = () => {\r\n    this.props.navigation.navigate('GiraffeScreen');\r\n  };\r\n  goToWildBeestScreen = () => {\r\n    this.props.navigation.navigate('WildBeestScreen');\r\n  };\r\n  goToLeopardScreen = () => {\r\n    this.props.navigation.navigate('LeopardScreen');\r\n  };\r\n  render() {\r\n    return (\r\n     <ScrollView style={styles.Container}>\r\n        <Text style={styles.Text2}>\r\n          AFRICA\r\n        </Text>\r\n        <Text style={{ color: 'black', fontSize: RFValue(20), fontWeight: 'bold' }}>\r\n          Click on the name of the animal you wish to view{' '}\r\n        </Text>\r\n        \r\n        <Image\r\n          source={require('../images/hippo.jpg')}\r\n          style={{\r\n            width: null,\r\n            height: 250,\r\n            resizeMode: 'center',\r\n          }}></Image>\r\n        <TouchableOpacity\r\n          style={{\r\n            alignSelf: 'center',\r\n            marginBottom:20,\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'yellow',\r\n            width: RFPercentage(25),\r\n            alignItems: 'center',\r\n          }}\r\n          onPress={() => this.goToHippoScreen()}>\r\n          <Text style={styles.Text}>HIPPOPOTAMUS</Text>\r\n        </TouchableOpacity>\r\n        <Image\r\n          source={require('../images/girr.jpg')}\r\n          style={{\r\n            width: RFValue(200),\r\n            height: RFValue(330),\r\n            resizeMode: 'stretch',\r\n            alignSelf:'center'\r\n          }}></Image>\r\n        <TouchableOpacity\r\n          style={{\r\n            alignSelf: 'center',\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'yellow',\r\n            width: RFPercentage(20),\r\n            alignItems: 'center',\r\n            marginTop:RFValue(15),\r\n            marginBottom:RFValue(15)\r\n          }}\r\n          onPress={() => this.goToGiraffeScreen()}>\r\n          <Text style={styles.Text}>GIRRAFFE</Text>\r\n        </TouchableOpacity>\r\n        <Image\r\n          source={require('../images/wbeest.jpg')}\r\n         style={{\r\n            width: RFValue(270),\r\n            height: RFValue(260),\r\n            resizeMode: 'stretch',\r\n            alignSelf:'center'\r\n          }}></Image>\r\n        <TouchableOpacity\r\n         style={{\r\n            alignSelf: 'center',\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'yellow',\r\n            width: RFPercentage(20),\r\n            alignItems: 'center',\r\n            marginTop:RFValue(20)\r\n          }}\r\n          onPress={() => this.goToWildBeestScreen()}>\r\n          <Text style={styles.Text}>WILD BEEST</Text>\r\n        </TouchableOpacity>\r\n        <Image\r\n          source={require('../images/leop.jpg')}\r\n          style={{\r\n            width: null,\r\n            height: 200,\r\n            resizeMode: 'center',\r\n          }}></Image>\r\n        <TouchableOpacity\r\n          style={{\r\n            alignSelf: 'center',\r\n            marginBottom:20,\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'yellow',\r\n            width: RFPercentage(20),\r\n            alignItems: 'center',\r\n          }}\r\n          onPress={() => this.goToLeopardScreen()}>\r\n            \r\n          \r\n          <Text style={styles.Text}>LEOPARD</Text>\r\n        </TouchableOpacity>\r\n         <TouchableOpacity\r\n          style={{\r\n            alignSelf: 'center',\r\n            marginTop: 50,\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'green',\r\n            width: RFPercentage(15),\r\n            alignItems: 'center',\r\n          }}\r\n          onPress={() => this.goToMapScreen()}>\r\n          <Text\r\n            style={{\r\n              fontFamily: 'sans-serif-condensed',\r\n              fontWeight: 'bold',\r\n              alignSelf: 'center',\r\n              justifyContent: 'center',\r\n              fontSize: RFValue(20),\r\n              color: 'white',\r\n            }}>\r\n            BACK\r\n          </Text>\r\n        </TouchableOpacity>\r\n      </ScrollView>\r\n    );\r\n  }\r\n}\r\nconst styles = StyleSheet.create({\r\n  Text: {\r\n    fontFamily: 'sans-serif-condensed',\r\n    fontWeight: 'bold',\r\n    alignSelf: 'center',\r\n    justifyContent: 'center',\r\n    fontSize: RFValue(20),\r\n    color: 'black',\r\n  },\r\n\r\n  Container: {\r\n    borderWidth: 1,\r\n    flex: 1,\r\n    backgroundColor: 'brown',\r\n  },\r\n   Text2: {\r\n    fontFamily: 'sans-serif-condensed',\r\n    fontWeight: 'bold',\r\n    alignSelf: 'center',\r\n    justifyContent: 'center',\r\n    fontSize: RFValue(50),\r\n    color: 'white',\r\n  }\r\n});\r\n"},"screens/AntarticaScreen.js":{"type":"CODE","contents":"import * as React from 'react';\r\nimport {\r\n  Text,\r\n  Image,\r\n  View,\r\n  StyleSheet,\r\n  TextInput,\r\n  ScrollView,\r\n  TouchableOpacity,\r\n  Header,\r\n} from 'react-native';\r\nimport MyHeader from '../components/MyHeader';\r\nimport { RFPercentage, RFValue } from 'react-native-responsive-fontsize';\r\nexport default class AntarticScreen extends React.Component {\r\n  goToMapScreen = () => {\r\n    this.props.navigation.navigate('MapScreen');\r\n  };\r\n  goToPenguinScreen = () => {\r\n    this.props.navigation.navigate('PenguinScreen');\r\n  };\r\n  goToSealScreen = () => {\r\n    this.props.navigation.navigate('SealScreen');\r\n  };\r\n  goToBlueWhaleScreen = () => {\r\n    this.props.navigation.navigate('BlueWhaleScreen');\r\n  };\r\n   goToSkuasScreen = () => {\r\n    this.props.navigation.navigate('SkuasScreen');\r\n  };\r\n  render() {\r\n    return (\r\n      <ScrollView style={styles.Container}>\r\n        <Text style={{fontSize: RFValue(50),alignSelf:'center'}}\r\n         >\r\n          ANTARTICA\r\n        </Text>\r\n        <Text style={{fontSize:RFValue(20), color: 'black' }}>\r\n          Click on the name of the animal you wish to view{' '}\r\n        </Text>\r\n        \r\n        <Image\r\n          source={require('../images/penguin.jpg')}\r\n          style={{\r\n            width: null,\r\n            height: RFValue(250),\r\n            resizeMode: 'center',\r\n          }}></Image>\r\n        <TouchableOpacity\r\n          style={{\r\n            alignSelf: 'center',\r\n            marginTop: 50,\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'black',\r\n            width: RFPercentage(15),\r\n            alignItems: 'center',\r\n          }}\r\n          onPress={() => this.goToPenguinScreen()}>\r\n          <Text style={styles.Text}>PENGUIN</Text>\r\n        </TouchableOpacity>\r\n        <Image\r\n          source={require('../images/seal.jpg')}\r\n          style={{\r\n            width: RFPercentage(37.5),\r\n            height: RFPercentage(25),\r\n            resizeMode: 'stretch',\r\n            alignSelf:'center',\r\n            marginTop:30\r\n          }}></Image>\r\n        <TouchableOpacity \r\n          style={{\r\n            alignSelf: 'center',\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'black',\r\n            width: RFPercentage(15),\r\n            alignItems: 'center',\r\n          }}\r\n          onPress={() => this.goToSealScreen()}>\r\n          <Text style={styles.Text}>SEAL</Text>\r\n        </TouchableOpacity>\r\n        <Image\r\n          source={require('../images/bwhale.jpg')}\r\n         style={{\r\n            width: RFValue(280),\r\n            height: RFValue(190),\r\n            resizeMode: 'stretch',\r\n            alignSelf:'center'\r\n          }}></Image>\r\n        <TouchableOpacity\r\n         style={{\r\n            alignSelf: 'center',\r\n            marginTop: 20,\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'black',\r\n            width: RFPercentage(25),\r\n            alignItems: 'center',\r\n          }}\r\n          onPress={() => this.goToBlueWhaleScreen()}>\r\n          <Text style={styles.Text}>BLUE WHALE</Text>\r\n        </TouchableOpacity>\r\n        <Image\r\n          source={require('../images/squas.jpg')}\r\n          style={{\r\n            width: RFValue(150),\r\n            height: RFValue(150),\r\n            resizeMode: 'stretch',\r\n            alignSelf:'center',\r\n            marginTop:40\r\n          }}></Image>\r\n        <TouchableOpacity\r\n          style={{\r\n            alignSelf: 'center',\r\n            marginTop: 50,\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'black',\r\n            width: RFPercentage(15),\r\n            alignItems: 'center',\r\n          }}\r\n          onPress={() => this.goToSkuasScreen()}>\r\n          <Text style={styles.Text}>SKUAS</Text>\r\n        </TouchableOpacity>\r\n        <TouchableOpacity\r\n          style={{\r\n            alignSelf: 'center',\r\n            marginTop: 50,\r\n            borderWidth: 2,\r\n            height: RFPercentage(5),\r\n            backgroundColor: 'green',\r\n            width: RFPercentage(15),\r\n            alignItems: 'center',\r\n          }}\r\n          onPress={() => this.goToMapScreen()}>\r\n          <Text\r\n            style={{\r\n              fontFamily: 'sans-serif-condensed',\r\n              fontWeight: 'bold',\r\n              alignSelf: 'center',\r\n              justifyContent: 'center',\r\n              fontSize: RFValue(20),\r\n              color: 'white',\r\n            }}>\r\n            BACK\r\n          </Text>\r\n        </TouchableOpacity>\r\n      </ScrollView>\r\n    );\r\n  }\r\n}\r\nconst styles = StyleSheet.create({\r\n  Text: {\r\n    fontFamily: 'sans-serif-condensed',\r\n    fontWeight: 'bold',\r\n    alignSelf: 'center',\r\n    justifyContent: 'center',\r\n    fontSize: RFValue(20),\r\n    color: 'white',\r\n  },\r\n\r\n  Container: {\r\n    borderWidth: 1,\r\n    flex: 1,\r\n    backgroundColor: 'white',\r\n  },\r\n});\r\n"},"screens/AsiaScreen.js":{"type":"CODE","contents":"import * as React from 'react';\r\nimport {\r\n  Text,\r\n  Image,\r\n  View,\r\n  StyleSheet,\r\n  TextInput,\r\n  TouchableOpacity,\r\n  Header,\r\n  ScrollView,\r\n} from 'react-native';\r\nimport MyHeader from '../components/MyHeader';\r\nimport { RFPercentage, RFValue } from 'react-native-responsive-fontsize';\r\n\r\nexport default class AsiaScreen extends React.Component {\r\n  goToMapScreen = () => {\r\n    this.props.navigation.navigate('MapScreen');\r\n  };\r\n  goToTigerScreen = () => {\r\n    this.props.navigation.navigate('TigerScreen');\r\n  };\r\n  goToLionScreen = () => {\r\n    this.props.navigation.navigate('LionScreen');\r\n  };\r\n  goToElephantScreen = () => {\r\n    this.props.navigation.navigate('ElephantScreen');\r\n  };\r\n  goToPythonScreen = () => {\r\n    this.props.navigation.navigate('PythonScreen');\r\n  };\r\n  render() {\r\n    return (\r\n      <ScrollView style={styles.Container}>\r\n        <Text style={styles.Text2}>ASIA</Text>\r\n        <Text style={{ color: 'yellow', fontSize: RFValue(20) }}>\r\n          Click on the name of the animal you wish to view{' '}\r\n        </Text>\r\n\r\n        <Image\r\n          source={require('../images/tiger.jpg')}\r\n          style={{\r\n            width: RFValue(190),\r\n            height: RFValue(250),\r\n            resizeMode: 'stretch',\r\n            alignSelf:'center'\r\n          }}></Image>\r\n        